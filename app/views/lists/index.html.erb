<h1>All lists</h1>
<br>
<p> <%= link_to 'Add list', new_list_path, class: 'btn btn-success'  %> </p>
<br>
<div class="d-flex justify-content-between">
  <% @lists.each do |list| %>
    <div class="text-bg-secondary bg-opacity-50 rounded p-2 mb-2">
      <h3><%= link_to list.name.capitalize, list_path(list), class: 'text-decoration-none text-light' %> </h3>
      <%# <p> <%= list.movie.title </p> %>
    </div>
  <% end %>
</div>
